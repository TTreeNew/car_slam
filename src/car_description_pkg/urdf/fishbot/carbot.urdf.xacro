<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="carbot">
<xacro:include filename="$(find car_description_pkg)/urdf/fishbot/base.urdf.xacro"/>
<xacro:include filename="$(find car_description_pkg)/urdf/fishbot/sensor/camera.urdf.xacro"/>
<xacro:include filename="$(find car_description_pkg)/urdf/fishbot/sensor/imu.urdf.xacro"/>
<xacro:include filename="$(find car_description_pkg)/urdf/fishbot/sensor/laser.urdf.xacro"/>

<xacro:include filename="$(find car_description_pkg)/urdf/fishbot/actuator/caster.urdf.xacro"/>
<xacro:include filename="$(find car_description_pkg)/urdf/fishbot/actuator/wheel.urdf.xacro"/>





<xacro:base_xacro length="0.12" radius="0.1"/>
<xacro:imu_xacro xyz="0 0 0.02"/>
<xacro:laser_xacro xyz="0 0 0.10"/>
<xacro:camera_xacro xyz="0.10 0 0.075"/>

<xacro:wheel_xacro wheel_name="left" xyz="0 0.10 -0.06" m="0.05" r="0.032" h="0.04" />
<xacro:wheel_xacro wheel_name="right" xyz="0 -0.10 -0.06" m="0.05" r="0.032" h="0.04" />

<xacro:caster_xacro caster_name="front" xyz="0.08 0.0 -0.076"/>
<xacro:caster_xacro caster_name="back" xyz="-0.08 0.0 -0.076"/>


<!--<xacro:include filename="$(find car_description_pkg)/urdf/fishbot/plugins/gazebo_control_plugin.xacro"/>
<xacro:gazebo_control_plugin />-->
<xacro:include filename="$(find car_description_pkg)/urdf/fishbot/carbot.ros2_control.xacro" />
<xacro:carbot_ros2_control />
<!--/home/tree/car_description/src/car_description_pkg/urdf/fishbot/carbot.ros2_control.xacro-->

<xacro:include filename="$(find car_description_pkg)/urdf/fishbot/plugins/gazebo_sensor_plugin.xacro"/>
<xacro:gazebo_sensor_plugin />


</robot>
